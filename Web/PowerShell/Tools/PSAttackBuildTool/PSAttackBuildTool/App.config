<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- Valid options for branch are: release, master, or dev -->
    <add key="branch" value="release"/>

    <!-- Valid options for arch are: x86 or x64 -->
    <add key="arch" value="x86"/>

    <!-- Valid options for obfuscatePowerShell are: true or false -->
    <add key="obfuscatePowerShell" value="true"/>
  </appSettings>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
